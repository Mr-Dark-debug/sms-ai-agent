# LLM Provider Configuration
# -------------------------
# Default provider: groq, openrouter, ollama
SMS_AGENT_LLM_PROVIDER=groq
SMS_AGENT_LLM_MODEL=llama-3.3-70b-versatile

# Groq API Key (Get at: https://console.groq.com/keys)
GROQ_API_KEY=your_groq_api_key_here

# OpenRouter API Key (Get at: https://openrouter.ai/keys)
OPENROUTER_API_KEY=your_openrouter_api_key_here

# LLM Parameters
SMS_AGENT_LLM_TEMPERATURE=0.7
SMS_AGENT_LLM_MAX_TOKENS=300

# SMS Configuration
# -----------------
SMS_AGENT_SMS_AUTO_REPLY_ENABLED=true
SMS_AGENT_SMS_AI_MODE_ENABLED=true

# Web UI Configuration
# --------------------
SMS_AGENT_UI_WEB_HOST=127.0.0.1
SMS_AGENT_UI_WEB_PORT=8080
SMS_AGENT_UI_WEB_DEBUG=false
